        <div id="content">
            <div class="container2">
	            <div class="row">
	                <div class="col-md-12">
	                    <h3 class="center capital f1 wow fadeInLeft" data-wow-duration="1s">Something went Wrong!</h3>
	                    <h1 id="error" class="center wow fadeInRight" data-wow-duration="1s">0</h1>
	                    <h2 class="center wow bounceIn" data-wow-delay="0.5s"><%= message %></h2>
	                    <pre class="wow bounceIn"><%= error.stack %></pre>
	                </div>
	            </div>
            </div>
        </div>

